#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir>

BUILD_DIR=$1

echo 'Installing pact_broker into:'

echo 'printing BUILD_DIR'
echo $BUILD_DIR

echo 'printing 1'
echo $1

curl -LO https://github.com/pact-foundation/pact-ruby-standalone/releases/download/v1.70.2/pact-1.70.2-linux-x86_64.tar.gz
tar xzf pact-1.70.2-linux-x86_64.tar.gz
